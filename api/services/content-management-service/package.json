{
  "name": "aws-rest-with-dynamodb",
  "version": "1.0.0",
  "description": "Serverless CRUD service exposing a REST HTTP interface",
  "scripts": {
    "start": "sls offline start --stage test",
    "db": "sls dynamodb start --stage test",
    "test": "sls invoke test --stage test",
    "test-function": "sls invoke test --stage test --function",
    "invoke": "sls invoke -f",
    "deploy": "serverless deploy --stage dev",
    "deploy-function": "serverless deploy --stage dev function --function"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "amazon-cognito-identity-js": "1.19.0",
    "jsonwebtoken": "7.4.3",
    "jwk-to-pem": "1.2.6",
    "lodash": "4.17.4",
    "node-cron": "1.2.1",
    "pusher": "1.5.1",
    "q": "1.5.0",
    "uuid": "2.0.3"
  },
  "devDependencies": {
    "serverless-dynamodb-local": "0.2.24",
    "serverless-export-env": "github:arabold/serverless-export-env",
    "serverless-mocha-plugin": "1.5.1",
    "serverless-offline": "3.15.3"
  }
}
